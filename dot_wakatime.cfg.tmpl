[settings]
debug = false
api_key = {{ .wakatime.api_key }}
exclude =
    ^COMMIT_EDITMSG$
    ^TAG_EDITMSG$
    ^/var/(?!www/).*
    ^/etc/
include =
    .*
include_only_with_project_file = false
offline = true
# proxy = https://user:pass@localhost:8080
no_ssl_verify = false
timeout = 30
hostname = {{ .hostname }}

[git]
disable_submodules = false
